{
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Launch Chrome [remote debugging]",
            "runtimeExecutable": "C:\\Program Files (x86)\\Google\\Chrome\\Application\\chrome.exe",
            "runtimeArgs": [
                "--nolazy",
                "--remote-debugging-port=9222",
                "--user-data-dir=C:\\chromeuserdir"
            ],
            "type": "chrome",
            "request": "launch",
            "port": 9222,
            "sourceMaps": true,
            "webRoot": "${workspaceRoot}/dist",
            "url": "http://127.0.0.1:8080"
        },
        {
            "name": "Attach to Chrome [remote debugging]",
            "type": "chrome",
            "request": "attach",
            "port": 9222,
            "webRoot": "${workspaceRoot}",
            "url": "http://127.0.0.1:8080"
        }        ,
        {
            "name": "Launch Chrome [firebase serve]",
            "type": "chrome",
            "request": "launch",
            "port": 9222,
            "webRoot": "${workspaceRoot}",
            "url": "http://localhost:5000"
        }        
    ]
}